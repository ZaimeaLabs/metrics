{
    "name": "zaimealabs/metrics",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "The ZaimeaLabs Metrics package.",
    "keywords": [
        "laravel",
        "metrics",
        "zaimealabs"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ZaimeaLabs\\Metrics\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZaimeaLabs\\Metrics\\MetricsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
